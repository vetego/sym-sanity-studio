    "src"
  ],
  "browserslist": "extends @sanity/browserslist-config",
  "keywords": [
    "sanity.io"
  ],
  "homepage": "https://github.com/sanity-io/signed-urls",
  "bugs": {
    "url": "https://github.com/sanity-io/signed-urls/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/signed-urls.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@sanity/pkg-utils": "^8.1.4",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.0"
  },
  "scripts": {
    "build": "pkg build --strict",
    "lint": "eslint .",
