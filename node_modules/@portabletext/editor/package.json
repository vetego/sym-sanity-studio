{
  "name": "@portabletext/editor",
  "version": "4.1.0",
  "description": "Portable Text Editor made in React",
  "keywords": [
    "sanity",
    "realtime",
    "content",
    "portable-text-editor",
    "structure",
    "collaborative",
    "editor",
    "text",
    "portable-text"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/portabletext/editor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/portabletext/editor.git",
    "directory": "packages/editor"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./behaviors": "./lib/behaviors/index.js",
    "./plugins": "./lib/plugins/index.js",
    "./selectors": "./lib/selectors/index.js",
    "./test": "./src/test/_exports/index.ts",
    "./test/vitest": "./src/test/vitest/_exports/index.ts",
    "./utils": "./lib/utils/index.js",
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@portabletext/to-html": "^5.0.0",
    "@sanity/schema": "^5.0.1",
    "@sanity/types": "^5.0.1",
    "@xstate/react": "^6.0.0",
    "debug": "^4.4.3",
    "slate": "^0.120.0",
    "slate-dom": "^0.119.0",
    "slate-react": "^0.120.0",
    "xstate": "^5.25.0",
    "@portabletext/block-tools": "^5.0.0",
    "@portabletext/keyboard-shortcuts": "^2.1.2",
    "@portabletext/markdown": "^1.1.1",
    "@portabletext/patches": "^2.0.3",
    "@portabletext/schema": "^2.1.1"
  },
  "devDependencies": {
    "@sanity/diff-match-patch": "^3.2.0",
    "@sanity/pkg-utils": "^10.2.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^20",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser": "^4.0.15",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-istanbul": "^4.0.15",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-formatter-gha": "^1.6.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rxjs": "^7.8.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.1.12",
    "vitest": "^4.0.15",
    "vitest-browser-react": "^2.0.2",
    "@portabletext/sanity-bridge": "2.0.0",
    "@portabletext/test": "^1.0.3",
    "racejar": "2.0.2"
  },
  "peerDependencies": {
    "@portabletext/sanity-bridge": "^2.0.0",
    "react": "^19.2.3",
    "rxjs": "^7.8.2"
  },
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  },
  "scripts": {
    "build": "pkg-utils build --strict --check --clean",
    "check:lint": "biome lint .",
    "check:react-compiler": "eslint --cache .",
    "check:types": "tsc",
    "check:types:watch": "tsc --watch",
    "clean": "del .turbo && del lib && del node_modules",
    "dev": "pkg-utils watch",
    "lint:fix": "biome lint --write .",
    "test": "vitest --run",
    "test:browser": "vitest --run --project browser",
    "test:browser:chromium": "vitest --run --project \"browser (chromium)\"",
    "test:browser:chromium:watch": "vitest --project \"browser (chromium)\"",
    "test:browser:firefox": "vitest --run --project \"browser (firefox)\"",
    "test:browser:firefox:watch": "vitest --project \"browser (firefox)\"",
    "test:browser:watch": "vitest --project browser",
    "test:browser:webkit": "vitest --run --project \"browser (webkit)\"",
    "test:browser:webkit:watch": "vitest --project \"browser (webkit)\"",
    "test:unit": "vitest --run --project unit",
    "test:unit:watch": "vitest --project unit",
    "test:watch": "vitest"
  }
}